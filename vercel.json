{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://1.235.57.64:8080/api/:path*",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "source": "/sam/:path*",
      "destination": "http://1.235.57.64:8001/sam/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}